03 053c #SKIN_CAP_VAL hex value in fF;
04 0001 #LT_APP_LCFG_TRIGGER_METHOD hex value; 0000 -> LT_APP_CAPSENSE_TUNED_TRIGGER, 0001 -> LT_APP_CAPSENSE_DISPLAY_TRIGGER, 0002 -> LT_APP_BUTTON_TRIGGER,  0003->LT_APP_INTERMITTENT_TRIGGER
